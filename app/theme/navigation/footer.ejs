<div id="footer">
  <% _.forEach(data.links.links.items, function(item) {  %>
    <a href="<%= data.links.links.functions.getRoute(item)%>" title="<%- item.description%>"><%- item.title%></a>
  <% }) %>
</div>
<style>
#footer {
  position: fixed;
  bottom:0;
  width: 100%;
  height: 20px;
  z-index: 10;
  border-top: #DCDCDC;
  background-color: #f2f3f4;
  font-size: 1.2em;
  display:flex;
  align-items: center;
  justify-content: center;
}
#footer > a {
  margin: 0 1%;
}
</style>
