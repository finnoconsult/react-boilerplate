{
  "name": "_FinnoProject",
  "version": "0.0.1",
  "description": "React project boilerplate template",
  "main": "start.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --content-base build/ --config webpack.config.js --hot --inline",
    "test": "jest",
    "_prebuild": "yarn test",
    "build": "mkdir ./Release; mv -f ./build/*.js ./Release; rm -rf ./build/*.js.map; NODE_ENV=production webpack -p --config webpack.config.js",
    "fixindex": "sed 's/\"\\/cler-tutorial/\".\\/cler-tutorial/g' ./build/index.html >./build/_index.html; mv ./build/_index.html ./build/index.html",
    "fix200": "sed 's/\"\\/cler-tutorial/\".\\/cler-tutorial/g' ./build/200.html >./build/_200.html; mv ./build/_200.html ./build/200.html",
    "_predeploy": "yarn test",
    "surgedeploy": "surge ./build _FinnoProject.surge.sh",
    "deploy": "yarn build; yarn fixindex; yarn fix200; yarn surgedeploy",
    "zipclean": "mkdir ./Package; rm ./Package/*.zip",
    "ziprelease": "cd ./build; zip -r -9 ../Package/_FinnoProject_0.0.1.zip ./*; cd ..",
    "zipupload": "scp -r ./Package/* media@media.finnoconsult.at:/home/media/public_html/clients/_FinnoProject",
    "predownloads": "more ./build/index.html; echo 'PLEASE DON'T FORGET TO yarn build and DOUBLE CHECK relative links are working: ./ instead of / in *.html; read -n1 -r -p \"Press any key to continue...\" key",
    "downloads": "yarn zipclean; yarn fixindex; yarn fix200; yarn ziprelease; yarn zipupload"
  },
  "author": "Peter <peter.horvath@finnoconsult.at>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "breakpoint-sass": "^2.7.1",
    "css-loader": "^0.26.1",
    "deep-equal": "^1.0.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "jest-enzyme": "^2.1.0",
    "node-sass": "^4.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-swipeable": "^3.9.1",
    "react-test-renderer": "^15.4.2",
    "sass-bem": "^2.6.5",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.26",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "2.1.0-beta.12"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "classnames": "^2.2.5",
    "html-webpack-plugin": "2.22.0",
    "mobx": "^2.7.0",
    "mobx-react": "^4.0.3",
    "platform": "^1.3.4",
    "prop-types": "^0.2.0",
    "rc-queue-anim": "^0.13.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router-dom": "^4.2.2",
    "react-style-proptype": "^3.0.0"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleNameMapper": {
      "^.+\\.(s?css|less|svg)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "./app/SomeTest.spec.js"
    ]
  }
}
